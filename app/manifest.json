{
  "name": "Telegram",
  "description": "Telegram Web App.\nMore info & source code here: https://github.com/Doka-NT/webogram",
  "version": "0.5.5",
  "short_name": "Telegram",
  "manifest_version": 2,
  "sidebar_action": {
    "default_icon": "img/icons/icon128.png",
    "default_title": "Telegram Web",
    "default_panel": "index.html"
  },
  "permissions": [
    "notifications",
    "webview",
    "fileSystem",
    "storage",
    "unlimitedStorage",
    "fullscreen"
  ],
  "icons": {
    "16": "img/icons/icon16.png",
    "32": "img/icons/icon32.png",
    "60": "img/icons/icon60.png",
    "64": "img/icons/icon64.png",
    "90": "img/icons/icon90.png",
    "128": "img/icons/icon128.png"
  }
}
